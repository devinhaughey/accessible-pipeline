{
  "name": "accessible-pipeline",
  "version": "0.0.1",
  "description": "Acessibility checks in CI, made easy",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "single": "LOG_LEVEL=trace TS_NODE_PRETTY=true ts-node index.ts",
    "production:single": "NODE_ENV=production LOG_LEVEL=info TS_NODE_PRETTY=true ts-node index.ts",
    "streaming": "LOG_LEVEL=trace TS_NODE_PRETTY=true ts-node index.ts --streaming",
    "production:streaming": "NODE_ENV=production LOG_LEVEL=info TS_NODE_PRETTY=true ts-node index.ts --streaming",
    "report": "TS_NODE_PRETTY=true ts-node reporter.tsx",
    "watch": "tsc --watch --pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fpapado/accessible-pipeline.git"
  },
  "keywords": [
    "a11y",
