  "bugs": {
    "url": "https://github.com/fpapado/accessible-pipeline/issues"
  },
  "homepage": "https://github.com/fpapado/accessible-pipeline#readme",
  "dependencies": {
    "@types/cheerio": "^0.22.10",
    "@types/pino": "^5.8.5",
    "@types/puppeteer": "^1.12.1",
    "@types/react": "^16.8.6",
    "@types/split2": "^2.1.6",
    "axe-puppeteer": "^1.0.0",
    "cheerio": "^1.0.0-rc.2",
    "fast-json-parse": "^1.0.3",
    "ink": "^2.0.5",
    "matchit": "^1.0.7",
    "pino": "^5.11.1",
    "pino-pretty": "^2.5.0",
    "puppeteer": "^1.12.2",
    "react": "^16.8.4",
    "split2": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  }
